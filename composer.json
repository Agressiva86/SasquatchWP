{
  "name": "spidersAgency/SasquatchWP_starter",
  "type": "project",
  "version": "1.0.5",
  "license": "MIT",
  "description": "Sasquatch WP boilerplate",
  "homepage": "https://spiders.agency",
  "authors": [
    {
      "name": "Maciej Palmowski",
      "email": "m.palmowski@spiders.agency",
      "homepage": "https://pandify.pl"
    }
  ],
  "keywords": [
    "bedrock", "roots", "wordpress", "stack", "composer", "wp", "timber"
  ],
  "config": {
    "preferred-install": "dist"
  },
  "repositories": [
    {
     "type": "package",
     "package": {
       "name": "advanced-custom-fields/advanced-custom-fields-pro",
       "version": "5.6.10",
       "type": "wordpress-plugin",
       "dist": {
         "type": "zip",
         "url": "https://connect.advancedcustomfields.com/index.php?p=pro&a=download"
       },
       "require": {
         "philippbaschke/acf-pro-installer": "^1.0",
         "composer/installers": "*"
       }
     }
    },
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.6",
    "composer/installers": "~1.0.25",
    "vlucas/phpdotenv": "^2.0.1",
    "johnpbloch/wordpress": "4.9.6",
    "oscarotero/env": "^1.1.0",
    "roots/wp-password-bcrypt": "1.0.0",
    "wp-sync-db/wp-sync-db": "dev-master",
    "wp-sync-db/wp-sync-db-media-files": "dev-master",
    "wpackagist-plugin/w3-total-cache": "*",
    "wpackagist-plugin/wp-deferred-javascripts": "*",
    "wpackagist-plugin/wordpress-seo": "*",
    "wpackagist-plugin/google-analytics-for-wordpress":"*",
    "wpackagist-plugin/regenerate-thumbnails":"*",
    "advanced-custom-fields/advanced-custom-fields-pro": "*",
    "soberwp/models": "1.0.6"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.5.1"
  },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "test": [
      "vendor/bin/phpcs"
    ]
  }
}
